<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>DomainDrivenGameEngine.Media.ImageSharp</PackageId>
    <Description>SixLabors.ImageSharp implementations of source interfaces defined in the DomainDrivenGameEngine.Media package.</Description>
    <Version>0.1.2</Version>
    <Authors>Preston Chaderton</Authors>
    <Company>Preston Chaderton</Company>
    <RepositoryUrl>https://github.com/pchaderton/DomainDrivenGameEngine.Media.ImageSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="DomainDrivenGameEngine.Media" Version="0.1.8" />
    <PackageReference Include="DomainDrivenGameEngine.RuleSet" Version="0.1.2">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="SixLabors.ImageSharp" Version="1.0.1" />
  </ItemGroup>

</Project>
